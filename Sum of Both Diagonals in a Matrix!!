#include <stdio.h>
int main() {
    int N;
    scanf("%d", &N);
    int matrix[100][100];
    long long primarySum = 0;
    long long secondarySum = 0;
    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++) {
            scanf("%d", &matrix[i][j]);
            if (i == j) {
                primarySum += matrix[i][j];
            }
            if (i + j == N - 1) {
                secondarySum += matrix[i][j];
            }
        }
    }
    long long totalSum = primarySum + secondarySum;
    if (N % 2 == 1) {
        totalSum -= matrix[N / 2][N / 2];
    }
    printf("Sum of Primary Diagonal: %lld\n", primarySum);
    printf("Sum of Secondary Diagonal: %lld\n", secondarySum);
    printf("Total Sum of Both Diagonals: %lld\n", totalSum);
    return 0;
}
